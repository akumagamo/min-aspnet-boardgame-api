<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="boardgames">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="boardgame" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="yearpublished" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
              <xs:element name="minplayers" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
              <xs:element name="maxplayers" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
              <xs:element name="playingtime" type="xs:string" minOccurs="0" msdata:Ordinal="3" />
              <xs:element name="minplaytime" type="xs:string" minOccurs="0" msdata:Ordinal="4" />
              <xs:element name="maxplaytime" type="xs:string" minOccurs="0" msdata:Ordinal="5" />
              <xs:element name="age" type="xs:string" minOccurs="0" msdata:Ordinal="6" />
              <xs:element name="description" type="xs:string" minOccurs="0" msdata:Ordinal="8" />
              <xs:element name="thumbnail" type="xs:string" minOccurs="0" msdata:Ordinal="9" />
              <xs:element name="image" type="xs:string" minOccurs="0" msdata:Ordinal="10" />
              <xs:element name="name" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="name_Text" msdata:Ordinal="2">
                    <xs:extension base="xs:string">
                      <xs:attribute name="primary" type="xs:string" />
                      <xs:attribute name="sortindex" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamepublisher" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamepublisher_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamepodcastepisode" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamepodcastepisode_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamehonor" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamehonor_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamefamily" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamefamily_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamemechanic" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamemechanic_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamecategory" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamecategory_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamedesigner" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamedesigner_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgameversion" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgameversion_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgamesubdomain" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgamesubdomain_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="boardgameartist" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="boardgameartist_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="cardset" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="cardset_Text" msdata:Ordinal="1">
                    <xs:extension base="xs:string">
                      <xs:attribute name="objectid" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="poll" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="results" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="result" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:attribute name="value" type="xs:string" />
                              <xs:attribute name="numvotes" type="xs:string" />
                              <xs:attribute name="level" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="numplayers" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="name" type="xs:string" />
                  <xs:attribute name="title" type="xs:string" />
                  <xs:attribute name="totalvotes" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="objectid" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="termsofuse" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="boardgames" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>